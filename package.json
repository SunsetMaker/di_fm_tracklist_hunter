{
  "name": "di_fm_tracklist_hunter",
  "version": "0.0.1",
  "author": "Sunset Maker <sunsetmaker@yandex.ru>",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development node --loader ts-node/esm ./src/index.ts",
    "start": "node --loader ts-node/esm ./src/index.ts",
    "build": "rollup -c",
    "lint": "eslint --ext=ts,tsx src",
    "lint:fix": "eslint --ext=ts,tsx src --fix",
    "format": "yarn prettier-standard **/*.{ts,tsx} typescript --write",
    "prepare": "husky install"
  },
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "dependencies": {
    "config": "^3.3.7",
    "ghost-cursor": "^1.1.10",
    "mongoose": "^6.2.8",
    "puppeteer": "^13.5.1",
    "puppeteer-extra": "^3.2.3",
    "puppeteer-extra-plugin-stealth": "^2.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.8",
    "@babel/node": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "@svgr/rollup": "^6.2.1",
    "@types/config": "^0.0.41",
    "@types/node": "^17.0.23",
    "@types/puppeteer": "^5.4.5",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.12.0",
    "eslint-config-next": "^12.1.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-prettier-standard": "^4.0.1",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^7.0.4",
    "nano-staged": "^0.6.0",
    "prettier": "^2.6.1",
    "prettier-config-standard": "^5.0.0",
    "prettier-standard": "^16.4.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "files": [
    "dist"
  ],
  "nano-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier-standard typescript --write",
      "eslint --ext=ts,tsx"
    ]
  }
}
